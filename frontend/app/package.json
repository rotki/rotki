{
  "name": "rotki",
  "version": "1.24.1",
  "description": "A portfolio tracking, asset analytics and tax reporting application specializing in Cryptoassets that protects your privacy",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "serve": "node scripts/serve.js --web",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test:unit": "cross-env TZ=UTC vitest --dir tests/unit --coverage",
    "test:e2e": "cross-env-shell dotenv -e .env.e2e 'start-server-and-test serve http-get://localhost:8080 \"npm run cypress:open -- $ARGS\"'",
    "lint": "eslint ./ --ext .js,.ts,.vue,.json",
    "electron:build": "npm run build && electron-builder build --config .electron-builder.config.js",
    "electron:serve": "node scripts/serve.js",
    "i18n:report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|js|vue)' --languageFiles './src/locales/**/*.json'",
    "lint:check": "eslint ./ --no-fix --max-warnings 0  --ext .js,.ts,.vue,.json",
    "preinstall": "node ../check-versions.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "serve:backend": "node start-backend.js",
    "lint:style": "stylelint 'src/**/*.{vue,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{vue,scss}'",
    "test:e2e-ci": "cross-env-shell dotenv -e .env.e2e 'start-server-and-test serve http-get://localhost:8080 \"npm run cypress:run -- $ARGS\"'",
    "test:integration": "cross-env-shell start-server-and-test serve:backend http://localhost:22221/api/1/ping test:e2e",
    "test:integration-ci": "cross-env-shell start-server-and-test serve:backend http://localhost:22221/api/1/ping test:e2e-ci"
  },
  "main": "dist/background.js",
  "dependencies": {
    "@rotki/common": "1.0.0",
    "@vue/composition-api": "1.4.6",
    "@vueuse/core": "8.0.0",
    "axios": "0.26.0",
    "chart.js": "2.9.4",
    "cleave.js": "1.6.0",
    "dayjs": "1.10.8",
    "electron-devtools-installer": "3.2.0",
    "electron-updater": "4.6.5",
    "electron-window-state": "5.0.3",
    "ethereum-blockies-base64": "1.0.2",
    "lodash": "4.17.21",
    "loglevel": "1.8.0",
    "pinia": "2.0.11",
    "register-service-worker": "1.7.2",
    "roboto-fontface": "*",
    "tasklist": "5.0.0",
    "typeface-roboto-mono": "1.1.13",
    "vue": "2.6.14",
    "vue-class-component": "7.2.6",
    "vue-i18n": "8.27.0",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.5.3",
    "vuetify": "2.6.3",
    "vuex": "3.6.2",
    "vuex-persistedstate": "3.2.1",
    "zod": "3.12.0"
  },
