{
  "name": "rotki",
  "version": "1.29.0",
  "description": "A portfolio tracking, asset analytics and tax reporting application specializing in Cryptoassets that protects your privacy",
  "license": "AGPL-3.0",
  "homepage": "https://rotki.com",
  "repository": "https://github.com/rotki/rotki",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "dist/background.js",
  "scripts": {
    "build": "pnpm run typecheck && pnpm run build:app",
    "build:app": "cross-env NODE_ENV=production node scripts/build.js",
    "build:preview": "pnpm run build:app && vite preview",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "electron:build": "pnpm run build && pnpm run electron:package",
    "electron:package": "electron-builder build --config .electron-builder.config.js",
    "electron:serve": "node scripts/serve.js",
    "i18n:report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|js|vue)' --languageFiles './src/locales/**/*.json'",
    "lint:style": "stylelint 'src/**/*.{vue,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{vue,scss}'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "serve": "node scripts/serve.js --web",
    "serve:backend": "node scripts/start-backend.js",
    "test:integration": "node scripts/e2e.js",
    "test:integration-ci": "node scripts/e2e.js --ci",
    "test:unit": "cross-env TZ=UTC VITE_TEST=true vitest --dir tests/unit --coverage",
    "test:unit:run": "cross-env TZ=UTC VITE_TEST=true vitest run --dir tests/unit --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "optionalDependencies": {
    "@babel/core": "7.22.1",
    "@babel/preset-env": "7.22.4",
    "@cypress/code-coverage": "3.10.7",
    "babel-loader": "8.3.0",
    "webpack": "5.85.0"
  },
  "dependencies": {
    "@rotki/common": "workspace:*",
    "@rotki/eslint-config-ts": "1.0.6",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.2",
