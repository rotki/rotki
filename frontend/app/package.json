{
  "name": "rotki",
  "version": "1.34.0",
  "description": "A portfolio tracking, asset analytics and tax reporting application specializing in Cryptoassets that protects your privacy",
  "type": "module",
  "license": "AGPL-3.0",
  "homepage": "https://rotki.com",
  "repository": "https://github.com/rotki/rotki",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "dist/background.js",
  "scripts": {
    "build": "pnpm run typecheck && pnpm run build:app",
    "build:app": "cross-env NODE_ENV=production node scripts/build.js",
    "build:preview": "pnpm run build:app && vite preview",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headed",
    "electron:build": "pnpm run build && pnpm run electron:package",
    "electron:package": "electron-builder build --config .electron-builder.config.cjs --publish onTag",
    "electron:serve": "node scripts/serve.js",
    "i18n:report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|js|vue)' --languageFiles './src/locales/**/*.json'",
    "link:verify": "ts-node scripts/check-external-links.ts",
    "lint:style": "stylelint 'src/**/*.{vue,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{vue,scss}'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "serve": "node scripts/serve.js --web",
    "serve:backend": "node scripts/start-backend.js",
    "test:contract": "node scripts/contract.js",
    "test:integration": "node scripts/e2e.js",
    "test:integration-ci": "node scripts/e2e.js --ci",
    "test:unit": "cross-env TZ=UTC VITE_TEST=true vitest --dir tests/unit --coverage",
    "test:unit:run": "cross-env TZ=UTC VITE_TEST=true vitest run --dir tests/unit --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "optionalDependencies": {
    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.24.8",
    "@cypress/code-coverage": "3.12.42",
    "babel-loader": "9.1.3",
    "webpack": "5.93.0"
  },
  "dependencies": {
    "@rotki/common": "workspace:*",
    "@rotki/ui-library": "1.0.0-beta.10",
    "@types/lodash-es": "4.17.12",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.11.0",
    "@vueuse/math": "10.11.0",
    "@vueuse/shared": "10.11.0",
    "axios": "1.6.7",
    "bignumber.js": "9.1.2",
    "chart.js": "4.4.3",
    "chartjs-plugin-zoom": "2.0.0",
    "dayjs": "1.11.11",
    "electron-updater": "6.2.1",
    "electron-window-state": "5.0.3",
    "imask": "7.6.1",
    "lodash-es": "4.17.21",
    "loglevel": "1.9.1",
    "pinia": "2.1.7",
    "ps-list": "8.1.1",
    "register-service-worker": "1.7.2",
    "roboto-fontface": "*",
    "tasklist": "5.0.0",
    "theme-colors": "0.1.0",
    "typeface-roboto-mono": "1.1.13",
    "vanilla-jsoneditor": "0.23.7",
    "vue": "3.4.32",
    "vue-i18n": "9.13.1",
    "vue-i18n-bridge": "9.13.1",
    "vue-router": "4.4.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@electron/notarize": "2.3.2",
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@pinia/testing": "0.1.3",
    "@types/node": "20.14.11",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitest/coverage-v8": "2.0.3",
    "@vitest/ui": "2.0.3",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.5.1",
    "ajv": "8.17.1",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "cypress": "13.13.1",
    "dmg-license": "1.0.11",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.2",
    "electron": "31.2.1",
    "electron-builder": "24.13.3",
    "electron-store": "8.2.0",
    "eslint": "9.7.0",
    "flush-promises": "1.0.2",
    "msw": "2.3.1",
    "postcss": "8.4.39",
    "postcss-html": "1.7.0",
    "postcss-scss": "4.0.9",
    "sass": "1.77.8",
    "semver": "7.6.3",
    "source-map-support": "0.5.21",
    "start-server-and-test": "2.0.4",
    "stylelint": "16.7.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.4.1",
    "tailwindcss": "3.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "unplugin-auto-import": "0.18.0",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.3.4",
    "vite-plugin-checker": "0.7.2",
    "vite-plugin-istanbul": "6.0.2",
    "vite-plugin-pwa": "0.20.0",
    "vite-plugin-vue-devtools": "7.3.6",
    "vite-plugin-vue-layouts": "0.11.0",
    "vitest": "2.0.3",
    "vue-i18n-extract": "2.0.7",
    "vue-tsc": "2.0.26",
    "workbox-window": "7.1.0"
  }
}
