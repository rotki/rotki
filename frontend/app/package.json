{
  "name": "rotki",
  "version": "1.38.0",
  "description": "A portfolio tracking, asset analytics and tax reporting application specializing in Cryptoassets that protects your privacy",
  "type": "module",
  "license": "AGPL-3.0",
  "homepage": "https://rotki.com",
  "repository": "https://github.com/rotki/rotki",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "dist/main.js",
  "scripts": {
    "build": "run-p typecheck \"build:app {@}\" --",
    "build:app": "tsx scripts/build.ts",
    "build:preview": "pnpm run build:app && vite preview",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headed",
    "electron:build": "pnpm run build && pnpm run electron:package",
    "electron:package": "electron-builder build --config .electron-builder.config.cjs --publish onTag",
    "electron:serve": "tsx scripts/serve.ts",
    "link:verify": "tsx scripts/check-external-links.ts",
    "lint:style": "stylelint 'src/**/*.{vue,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{vue,scss}'",
    "postinstall": "electron-builder install-app-deps && node scripts/postinstall.js",
    "postuninstall": "electron-builder install-app-deps",
    "serve": "tsx scripts/serve.ts --web",
    "test:contract": "node scripts/contract.js",
    "test:integration": "tsx scripts/e2e.ts",
    "test:integration-ci": "tsx scripts/e2e.ts --ci",
    "test:unit": "vitest --coverage --exclude tests/contract/**",
    "test:unit:run": "vitest run --coverage --exclude tests/contract/**",
    "typecheck": "vue-tsc --build --force",
    "traces": "vue-tsc --build --force --generateTrace traces"
  },
  "optionalDependencies": {
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
